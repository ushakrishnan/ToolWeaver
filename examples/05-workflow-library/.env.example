# Azure OpenAI Configuration (if using LLM for dynamic workflows)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_KEY=your_key_here
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4o
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Workflow Configuration (optional - has defaults)
WORKFLOW_MAX_PARALLEL_STEPS=4
WORKFLOW_RETRY_FAILED_STEPS=true
WORKFLOW_MAX_RETRIES=3
WORKFLOW_STEP_TIMEOUT=30
WORKFLOW_SAVE_INTERMEDIATE=true
WORKFLOW_ENABLE_ROLLBACK=true

# Workflow Library Storage (optional)
WORKFLOW_LIBRARY_PATH=./workflows
WORKFLOW_AUTO_SAVE=true
